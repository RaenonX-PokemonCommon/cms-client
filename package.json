{
  "name": "@raenonx-pokemon/cms-client",
  "description": "JS / TS client for Strapi Rest API",
  "packageManager": "yarn@4.5.0",
  "version": "1.0.0",
  "private": true,
  "author": "Karthikeyan Mariappan",
  "repository": {
    "type": "git",
    "url": "https://github.com/RaenonX-PokemonCommon/cms-client"
  },
  "maintainers": [
    {
      "name": "RaenonX",
      "email": "raenonx0710@gmail.com"
    },
    {
      "name": "John Mitchell-Grant",
      "email": "jgrant_dev@icloud.com"
    }
  ],
  "license": "MIT",
  "type": "module",
  "files": [
    "dist",
    "src"
  ],
  "exports": {
    ".": {
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      }
    }
  },
  "source": "./src/index.ts",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "engines": {
    "node": "^22.x.x",
    "npm": "^10.x.x"
  },
  "scripts": {
    "dev": "rimraf dist && rollup --config --watch",
    "build": "rimraf dist && cross-env NODE_ENV=production rollup --config --compact",
    "lint": "eslint src --ext ts,tsx,js,jsx,cjs,mjs",
    "typecheck": "tsc"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "axios-date-transformer": "^1.2.1",
    "qs": "^6.13.0"
  },
  "devDependencies": {
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-json": "^6.1.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-swc": "^0.4.0",
    "@rollup/plugin-typescript": "^12.1.0",
    "@swc/core": "^1.7.28",
    "@types/node": "^22.7.4",
    "@types/qs": "^6.9.16",
    "ajv": "^8.17.1",
    "cross-env": "^7.0.3",
    "rimraf": "^6.0.1",
    "rollup": "^4.22.5",
    "rollup-plugin-dts": "^6.1.1",
    "ts-node": "^10.9.2",
    "type-fest": "^4.26.1",
    "typescript": "^5.6.2"
  }
}
